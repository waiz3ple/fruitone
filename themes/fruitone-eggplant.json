        "titleBar.inactiveForeground": "#bbbbbb"
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#546E7A"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "string constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#EEFFFF"
            }
        },
        {
            "name": "Colors",
            "scope": [
                "constant.other.color"
            ],
            "settings": {
                "foreground": "#FFFFFF"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#FF5370"
            }
        },
        {
            "name": "Keyword, Storage",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#C792EA"
            }
        },
        {
            "name": "Operators and Misc",
            "scope": [
                "keyword.control",
                "constant.other.color",
                "punctuation",
                "meta.tag",
                "punctuation.definition.tag",
                "punctuation.separator.inheritance.php",
                "punctuation.definition.tag.html",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html",
                "punctuation.section.embedded",
                "keyword.other.template",
                "keyword.other.substitution"
            ],
            "settings": {
                "foreground": "#89DDFF"
            }
        },
        {
            "name": "Tag",
            "scope": [
                "entity.name.tag",
                "meta.tag.sgml",
                "markup.deleted.git_gutter"
            ],
            "settings": {
                "foreground": "#F07178"
            }
        },
        {
            "name": "Function and Methods",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "variable.function",
                "support.function",
                "keyword.other.special-method"
            ],
            "settings": {
                "foreground": "#82AAFF"
            }
        },
        {
            "name": "Block Variables",
            "scope": [
                "meta.block variable.other"
            ],
            "settings": {
                "foreground": "#F07178"
            }
        },
        {
            "name": "Strings and Links",
            "scope": [
                "string",
                "string.other.link",
                "constant.other.symbol",
                "entity.other.inherited-class",
                "markup.heading",
                "markup.inserted.git_gutter"
            ],
            "settings": {
                "foreground": "#C3E88D"
            }
        },
        {
            "name": "Numbers and Constants",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant",
                "constant.character",
                "constant.escape",
                "variable.parameter",
                "keyword.other.unit"
            ],
            "settings": {
                "foreground": "#60A848"
            }
        },
        {
            "name": "Classes and Support",
            "scope": [
                "entity.name",
                "support.type",
                "support.class",
                "markup.changed.git_gutter"
            ],
            "settings": {
                "foreground": "#FFCB6B"
            }
        },
        {
            "name": "CSS Classes and IDs",
            "scope": [
                "entity.other.attribute-name.class",
                "entity.other.attribute-name.id"
            ],
            "settings": {
                "foreground": "#FFCB6B"
            }
        },
        {
            "name": "Regular Expressions",
            "scope": [
                "string.regexp",
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#89DDFF"
            }
        },
        {
            "name": "Markdown Headers",
            "scope": [
                "markup.heading",
                "markup.heading.markdown punctuation.definition.heading.markdown"
            ],
            "settings": {
                "foreground": "#C3E88D"
            }
        },
        {
            "name": "Bold and Italic",
            "scope": [
                "markup.bold",
                "markup.italic",
                "markup.bold markup.italic"
            ],
            "settings": {
                "fontStyle": "bold italic",
                "foreground": "#F07178"
            }
        },
        {
            "name": "Decorators",
            "scope": [
                "tag.decorator.js entity.name.tag.js",
                "tag.decorator.js punctuation.definition.tag.js"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#82AAFF"
            }
        },
        {
            "name": "JSON Keys",
            "scope": [
                "source.json support.type.property-name.json",
                "meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#C792EA"
            }
        },
        {
            "name": "HTML Attributes",
            "scope": [
                "text.html.basic entity.other.attribute-name.html",
                "text.html.basic entity.other.attribute-name"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#FFCB6B"
            }
        },
        {
            "name": "Markdown Links",
            "scope": [
                "string.other.link.title.markdown",
                "constant.other.reference.link.markdown"
            ],
            "settings": {
                "foreground": "#82AAFF"
            }
        }
    ]
}