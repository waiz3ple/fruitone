    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#FFA467"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "string constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#FFE7C2"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "constant.other.symbol"
            ],
            "settings": {
                "foreground": "#FF944D"
            }
        },
        {
            "name": "Numbers and Constants",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant"
            ],
            "settings": {
                "foreground": "#FF7A2F"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage.type"
            ],
            "settings": {
                "foreground": "#FFB84D"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#FFD7A5"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.control",
                "punctuation"
            ],
            "settings": {
                "foreground": "#FF944D"
            }
        },
        {
            "name": "Tags",
            "scope": [
                "entity.name.tag",
                "meta.tag"
            ],
            "settings": {
                "foreground": "#FF7A2F"
            }
        },
        {
            "name": "Class Names",
            "scope": [
                "entity.name.class"
            ],
            "settings": {
                "foreground": "#FFD7A5"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#FFB84D"
            }
        },
        {
            "name": "JSON Keys",
            "scope": [
                "source.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#FF7A2F"
            }
        },
        {
            "name": "Regular Expressions",
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#FFD7A5"
            }
        }
    ]
}