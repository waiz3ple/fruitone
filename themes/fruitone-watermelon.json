    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#88AA88"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "string constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#F5B7C0"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "constant.other.symbol"
            ],
            "settings": {
                "foreground": "#85C785"
            }
        },
        {
            "name": "Numbers and Constants",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant"
            ],
            "settings": {
                "foreground": "#FF5E72"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage.type"
            ],
            "settings": {
                "foreground": "#C792EA"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#82AAFF"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.control",
                "punctuation"
            ],
            "settings": {
                "foreground": "#FF5E72"
            }
        },
        {
            "name": "Tags",
            "scope": [
                "entity.name.tag",
                "meta.tag"
            ],
            "settings": {
                "foreground": "#FF5E72"
            }
        },
        {
            "name": "Class Names",
            "scope": [
                "entity.name.class"
            ],
            "settings": {
                "foreground": "#FFD580"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#85C785"
            }
        },
        {
            "name": "JSON Keys",
            "scope": [
                "source.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#FFD580"
            }
        },
        {
            "name": "Regular Expressions",
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#82AAFF"
            }
        }
    ]
}