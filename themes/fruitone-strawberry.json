    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#FF97A9"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "string constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#FFD9E0"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "constant.other.symbol"
            ],
            "settings": {
                "foreground": "#FF5A7A"
            }
        },
        {
            "name": "Numbers and Constants",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant"
            ],
            "settings": {
                "foreground": "#FF2E63"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage.type"
            ],
            "settings": {
                "foreground": "#FF97A9"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#FFD1E5"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.control",
                "punctuation"
            ],
            "settings": {
                "foreground": "#FF5A7A"
            }
        },
        {
            "name": "Tags",
            "scope": [
                "entity.name.tag",
                "meta.tag"
            ],
            "settings": {
                "foreground": "#FF2E63"
            }
        },
        {
            "name": "Class Names",
            "scope": [
                "entity.name.class"
            ],
            "settings": {
                "foreground": "#FFD1E5"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#FF5A7A"
            }
        },
        {
            "name": "JSON Keys",
            "scope": [
                "source.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#FF2E63"
            }
        },
        {
            "name": "Regular Expressions",
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#FFD1E5"
            }
        }
    ]
}