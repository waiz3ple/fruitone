        "editorGroupHeader.tabsBackground": "#3A2E2E",
        "editorGroup.border": "#397539",
        "editorWidget.background": "#5A3940",
        "editorWidget.foreground": "#FFFFFF",
        "editorWidget.border": "#397539",
        "list.activeSelectionBackground": "#4D944D",
        "list.activeSelectionForeground": "#FFFFFF",
        "list.hoverBackground": "#85C785",
        "list.hoverForeground": "#FFFFFF",
