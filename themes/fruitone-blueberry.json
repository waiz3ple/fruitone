    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#7286A0"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "string constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#AFC4E5"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "constant.other.symbol"
            ],
            "settings": {
                "foreground": "#6EA0FF"
            }
        },
        {
            "name": "Numbers and Constants",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant"
            ],
            "settings": {
                "foreground": "#5C80FF"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage.type"
            ],
            "settings": {
                "foreground": "#A082D9"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#78B2FF"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.control",
                "punctuation"
            ],
            "settings": {
                "foreground": "#5C80FF"
            }
        },
        {
            "name": "Tags",
            "scope": [
                "entity.name.tag",
                "meta.tag"
            ],
            "settings": {
                "foreground": "#A082D9"
            }
        },
        {
            "name": "Class Names",
            "scope": [
                "entity.name.class"
            ],
            "settings": {
                "foreground": "#FFD580"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#6EA0FF"
            }
        },
        {
            "name": "JSON Keys",
            "scope": [
                "source.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#A082D9"
            }
        },
        {
            "name": "Regular Expressions",
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#78B2FF"
            }
        }
    ]
}